                        grav = -0.05;
                    }
                    with(instance_place(x+224, y-2, Floating_Blocks_Object))
                    {
                        use_gravity = 1;
                        grav = -0.05;
                    }
                }
            }
        }
    }   
}
else if (stage1_cage_top &amp;&amp; stage1_cage_sides)
