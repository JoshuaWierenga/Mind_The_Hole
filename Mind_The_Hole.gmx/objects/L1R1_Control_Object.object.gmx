        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (Control_Object.playing)
{
    if (player_fallen == 0 &amp;&amp; round(Player_Object.y) &gt;= 480)
    {
        player_fallen = 1;
        with (Player_Object)
        {
            hp -= 1;
            hurt = 1;
            alarm[0] = 30;
        }
    }
    
    if (exit_fallen == 1 &amp;&amp; started_cage == 0 &amp;&amp; round(Player_Object.y) == 504 &amp;&amp; Player_Object.x &gt;= 64 &amp;&amp; Player_Object.x &lt;= 255)
    {
        started_cage = 1;
        with(instance_position(192, -608, L1R1_Cage_Top_Object))
        { 
            use_gravity = 1;
            alarm[0] = 20;
            alarm[1] = 40;
        }
        
        with(instance_position(64, 0, Grass_Floor_Object))
        {
            instance_destroy();
        }
        
        with(instance_position(288, 0, Grass_Floor_Object))
        {
            instance_destroy();
        }
        
        with(instance_position(65, 737, L1R1_Cage_Bottom_Object))
        {
            use_gravity = 1;
        }
    }
    
    if (cage_finished == 1 &amp;&amp; started_falling == 0)
    {
        started_falling = 1;    
        alarm[6] = 30;
    }  
    
    if (start_cage_fall == 1 &amp;&amp; cage_replaced == 0)
    {
        
        player_inside_cage = Player_Object.x &gt;= 96 &amp;&amp; Player_Object.x &lt;= 256 &amp;&amp; Player_Object.y &gt;= 480;
    
        cage_replaced = 1;
        
        if (player_inside_cage) Control_Object.allow_player_jump = false;
        
        for (i = 66; i &lt;= 290; i += 32)
        {
            for (j = 354; j &lt;= 706; j += 32)
            {

                if (instance_position(i, j, all) != inst_B3F0C3EF)
                {
                    with(instance_position(i, j, all))
                    {
                        instance_destroy();
                    }
                }
            }
        }
        
        instance_create(176, -32.125, L1R1_Cage_Holder_Object);
        instance_create(64, 256, L1R1_Cage_Body_Object);
    }
    
    if (cage_replaced == 1)
    {
        time_falling++;
        if (player_inside_cage) Player_Object.y += 3;
    }
    
    if (time_falling == 80)
    {
        if (player_inside_cage)
        {
            with (Player_Object)
            {
                persistent = 0;
            }
            transition(9,0, 120, c_white);
        }
        else
        {
           waiting_for_player = 1; 
        }
    }
    
    if (waiting_for_player)
    {
        if (Player_Object.y &gt;= 672)
        {   
            Player_Object.y = 0;
            transition(room+1,0);
        }
    }
}
</string>
